{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "lambda",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outExtension": {".js": ".mjs"}
  },
  "include": ["lambda/**/*.ts"]
}
